#include <iostream> 
using namespace std; 
class Rectangle 
{ 
public: 
    float area; 
    Rectangle()
    { 
        area = 0; 
    } 
    Rectangle(int l, int b) 
    { 
        area = l*b; 
    } 
    void displaydata() 
    { 
        cout << "Area:" << area << endl; 
    } 
}; 
int main() 
{
    Rectangle r(65, 98); 
    r.displaydata(); 
    return 0; 
}
