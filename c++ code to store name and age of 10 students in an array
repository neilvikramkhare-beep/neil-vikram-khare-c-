#include <iostream>
using namespace std;
class school
{
    string name;
    int age;
    void getdata()
    {
        cout<<"enter your name:\n";
        cin>>name;
        cout<<"enter your age:\n";
        cin>>age;
    }
    void putdata()
    {
        cout<<"Name:"<<name<<endl;
        cout<<"Age:"<<age<<endl;
    }
};
int main()
{
    school s[10];
    for(int i=1;i=10;i++)
        void getdata();
        void putdata();
    return 0;
}
