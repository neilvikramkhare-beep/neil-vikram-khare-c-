#include <iostream>
#include<unordered_map>
#include<map>
using namespace std;
int main()
 {
   unordered_map<int,string>pencil;
   map<string,int>pen;
   pen.insert({"John",100});
    pen.insert({"Joha",200});
    pencil.insert({100,"John"});
    pencil.insert({200,"Doe"});
    pencil.insert({100,"Sunny"});
    cout<<pencil[100]<<endl;
    cout<<pencil.at(200)<<endl;
    for(pair<string,int> p: pen )
    {
        cout<<p.first<<" and "<<p.second<<endl;
    }
    return 0;
}
