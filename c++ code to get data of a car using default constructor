#include <iostream>
#include<string>
using namespace std;
class Car
{
    public:
  Car()
  {
  getdata();
  }
    string colour;
   string brand;
    string model;
    int millege;
    void getdata()
    {
        colour="Red";
        brand="Toyota";
        model="Hybrid";
        millege=29000;
    }
    void displaydata()
    {
        cout<<"Colour:"<<colour<<endl;
        cout<<"Brand:"<<brand<<endl;
        cout<<"Model:"<<model<<endl;
        cout<<"Millege:"<<millege<<endl;
    }
};
int main()
{
   Car c;
   c.displaydata();
    return 0;
}
    
