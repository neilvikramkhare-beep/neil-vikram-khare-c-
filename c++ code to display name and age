#include<iostream>
using namespace std;
class school
{
string students[10];
int age;
string name;
public:
void getdata()
{
    std::cout<<"enter name:";
    std::cin>>name;
    std::cout<<"enter age:";
    std::cin>>age;
}
void putdata()
{
     cout<<"Name:"<<name<<endl;
        cout<<"Age:"<<age<<endl;
        cout<<endl;
  }
};
int main()
{
    school s;
    s.getdata();
    s.putdata();
    return 0;
}
