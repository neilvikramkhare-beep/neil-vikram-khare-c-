#include <iostream>
#include <algorithm> 
#include <vector>    
using namespace std;
int main()
{
   vector<int> a = {12, 35, 1, 10, 34, 1};
    sort(a.begin(), a.end());
    int n = a.size();
    if (n < 2) 
    {
     cout << "Array must contain at least two elements." <<endl;
        return 1; 
    }
    int secondLargest = a[n - 2];
cout << "The second largest element is: " << secondLargest << endl;
    return 0;
}
