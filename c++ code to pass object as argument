#include<iostream>
using namespace std;
class Example
{
    public:
    int a;
    int b;
    int c;
    Example add(Example Ea, Example Eb)
    {
        Example Ec;
        Ec.a=Ec.a+Ec.b+Ec.c;
        return Ec;
    }
};
int main()
{
    Example E1,E2,E3;
    E1.a=65;
    E2.a=68;
    E3.a=54;
    cout<<"Initial values:";
    cout<<"\nValue of object 1:"<<E1.a;
    cout<<"\nValue of object 2:"<<E2.a;
    cout<<"\nValue of object 3:"<<E3.a;
    cout<<"\n";
    E3=E3.add(E1,E2);
    cout<<"New values:";
    cout<<"\nValue of object 1:"<<E1.a;
    cout<<"\nValue of object 2:"<<E2.a;
    cout<<"\nValue of object 3:"<<E3.a;
    cout<<"\n";
    return 0;

}
