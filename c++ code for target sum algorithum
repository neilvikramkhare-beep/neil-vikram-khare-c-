#include <iostream>
#include <vector>
#include <utility> 
std::vector<std::pair<int, int>> findPairsWithTargetSum(const std::vector<int>& arr, int target) {
    std::vector<std::pair<int, int>> pairs; 
    for (size_t i = 0; i < arr.size(); ++i) {
        for (size_t j = i + 1; j < arr.size(); ++j) { 
            if (arr[i] + arr[j] == target) {
                pairs.push_back(std::make_pair(arr[i], arr[j]));
            }
        }
    }
    return pairs;
}
int main() {
    std::vector<int> numbers = {2, 4, 5, 3, 6, 8};
    int targetSum = 9;
    std::vector<std::pair<int, int>> resultPairs = findPairsWithTargetSum(numbers, targetSum);
    std::cout << "Pairs that sum up to " << targetSum << " are:" << std::endl;
    if (resultPairs.empty()) {
        std::cout << "No pairs found." << std::endl;
    } else {
        for (const auto& pair : resultPairs) {
            std::cout << "(" << pair.first << ", " << pair.second << ")" << std::endl;
        }
    }
    std::vector<int> anotherNumbers = {1, 2, 3, 4, 5};
    int anotherTargetSum = 7;
    std::vector<std::pair<int, int>> anotherResult = findPairsWithTargetSum(anotherNumbers, anotherTargetSum);
    std::cout << "\nPairs that sum up to " << anotherTargetSum << " in the second array are:" << std::endl;
    if (anotherResult.empty()) {
        std::cout << "No pairs found." << std::endl;
    } else {
        for (const auto& pair : anotherResult) {
            std::cout << "(" << pair.first << ", " << pair.second << ")" << std::endl;
        }
    }
    return 0;
}
