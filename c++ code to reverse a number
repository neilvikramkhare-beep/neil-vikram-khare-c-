#include <iostream>
int main()
 {
    int n, reversed_number = 0, remainder;
    std::cout << "Enter an integer: ";
    std::cin >> n;
    while (n != 0)
     {
        remainder = n % 10;
        reversed_number = reversed_number * 10 + remainder;
        n /= 10;
    }
    std::cout << "Reversed Number = " << reversed_number << std::endl;
    return 0;
}
