#include <iostream>
using namespace std;
class Length
{
    protected:
    int l;
    public:
    void input()
    {
        cout<<"enter the length:";
        cin>>l;
    }
};
class Breath
{
    protected:
    int b;
    public:
    void getdata()
    {
        cout<<"enter the breath:";
        cin>>b;
    }
};
class Area:public Length, public Breath
{
    public:
    int a;
    void calc()
    {
        a=l*b;
    }
    void display()
    {
        cout<<"Area:"<<a<<endl;
    }
};
int main()
{
    Area a1[10];
    int i;
    cout<<"area for 10 rectangles:"<<endl;
    for(int i=0;i<10;i++)
    {
        a1[i].input();
        a1[i].getdata();
        a1[i].calc();
        a1[i].display();
    }
    return 0;
}
