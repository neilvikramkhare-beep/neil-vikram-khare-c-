#include <iostream>
using namespace std;
template<class X>
void swapargs(X&a, X&b) 
{
    X temp;
    temp=a;
    a=b;
    b=temp;
}
int main()
{
    int i = 10;
    int j = 56;
    float m = 7.5;
    float n = 6.5;
   cout<<"original i,j:"<<i<<' '<<j<<"\n";
    cout<<"original m,n:"<<m<<' '<<n<<"\n";
    swapargs(i,j);
    swapargs(m,n);
    cout<<"swapped i,j:"<<i<<' '<<j<<"\n";
    cout<<"swapped m,n:"<<m<<' '<<n<<"\n";
    return 0;
}
