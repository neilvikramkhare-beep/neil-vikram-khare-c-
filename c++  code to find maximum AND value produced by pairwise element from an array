#include <iostream>
#include <algorithm> 
using namespace std;
int main() 
{
    int a[] = {4, 8, 6, 2, 12, 10};
    int n = sizeof(a) / sizeof(a[0]);
    int maxAND = 0;
    for (int i = 0; i < n; i++) 
    {
        for (int j = i + 1; j < n; j++) 
        {
            int currentAND = (a[i] & a[j]);
            if (currentAND > maxAND) 
            {
                maxAND = currentAND;
            }
        }
    }
    cout << "Array: ";
    for(int i=0; i<n; i++) 
    cout << a[i] << " ";
    cout << "\nMaximum AND Value: " << maxAND << endl;
    return 0;
}
