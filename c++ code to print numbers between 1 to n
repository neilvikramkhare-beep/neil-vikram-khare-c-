#include <iostream>
using namespace std;
int main()
{
    int n,i;
    cout << "Enter a positive integer: ";
    cin >> n;
    if (n <= 0)
    {
       cout << "Please enter a positive integer." << endl;
       return 1; 
    }
cout << "Numbers from 1 to " << n << " are: ";
    for (i = 1; i <= n; i++) 
    {
     cout << i << " ";
    }
cout <<endl; 
    return 0; 
}

