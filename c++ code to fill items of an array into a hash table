#include <iostream>
#include <vector>
using namespace std;
int hashFunction(int key, int size) {
    return key % size;
}
void insert(vector<int>& hashTable, int value, int size) {
    int hashIndex = hashFunction(value, size);
    while (hashTable[hashIndex] != -1) {
        hashIndex = hashFunction(hashIndex + 1, size);
    }
    hashTable[hashIndex] = value;
}
void fillHashTable(vector<int>& hashTable, const vector<int>& dataArray, int size) {
    for (int value : dataArray) {
        insert(hashTable, value, size);
    }
}
void printHashTable(const vector<int>& hashTable, int size) {
    for (int i = 0; i < size; ++i) {
        cout << "hashTable[" << i << "] = " << hashTable[i] << endl;
    }
}
int main() 
{
    vector<int> dataArray = {10, 4, 2, 30, 12, 17};
    const int SIZE = 7; 
    vector<int> hashTable(SIZE, -1);
    fillHashTable(hashTable, dataArray, SIZE);
    cout << "Hash Table after filling:" << endl;
    printHashTable(hashTable, SIZE);
    return 0;
}
