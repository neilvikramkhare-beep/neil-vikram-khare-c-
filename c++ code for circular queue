#include <iostream>
using namespace std;
class Queue {
    int arr[100];
    int front, rear, size;
public:
    Queue(int s) {
        size = s;
        front =-1;
        rear = -1;
    }
    void enqueue(int x) {
        if (rear == size - 1)
            cout << "Queue Overflow\n";
        else
            arr[++rear] = x;
    }
    void dequeue() {
        if (front > rear)
            cout << "Queue Underflow\n";
        else
            front++;
    }
    void display() {
        if (front > rear)
            cout << "Queue is empty\n";
        else {
            cout << "Queue: ";
            for (int i = front; i <= rear; i++)
                cout << arr[i] << " ";
            cout << "\n";
        }
    }
};
int main() {
    int n;
    cout << "Enter size of queue: ";
    cin >> n;
    Queue q(n);
    q.enqueue(10);
    q.enqueue(20);
    q.enqueue(30);
    q.display();
    q.dequeue();
    q.display();
    return 0;
}
