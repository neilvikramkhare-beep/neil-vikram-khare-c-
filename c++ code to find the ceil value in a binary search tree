#include <bits/stdc++.h>
using namespace std;
struct Node 
{
    int data;
    Node* left;
    Node* right;
    Node(int val) 
    {
        data = val;
        left = right = NULL;
    }
};
class Binarytree
{
public:
    int ceil(Node* root, int x)
    {
        int ceilValue = -1;
        while (root != NULL)
        {
            if (root->data == x)
            {
                return root->data;  
            }
            else if (root->data < x)
            {
                root = root->right;  
            }
            else
            {
                ceilValue = root->data; 
                root = root->left;      
            }
        }
        return ceilValue;
    }
};
Node* insert(Node* root, int key) 
{
    if (root == NULL)
        return new Node(key);
    if (key < root->data)
        root->left = insert(root->left, key);
    else
        root->right = insert(root->right, key);
    return root;
}
int main() 
{
    int n;
    cin >> n;
    Node* root = NULL;
    for (int i = 0; i < n; i++) 
    {
        int x;
        cin >> x;
        root = insert(root, x);
    }
    int key;
    cin >> key;
    Binarytree br;
    cout << br.ceil(root, key) << endl;
    return 0;
}
