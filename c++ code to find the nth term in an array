#include <iostream>
using namespace std;
int main() 
{
   int a[]={10,20,30,40,50,60,70};
   int size=sizeof(a)/sizeof(a[0]);
   int n=4;
   if(n>=1&&n<=size)
   {
       int c=a[n-1];
       cout<<"The "<<n<<"th term of the array is:"<<c;
   }
   else
   {
       cout<<"Size exceeded.";
   }
    return 0;
}
