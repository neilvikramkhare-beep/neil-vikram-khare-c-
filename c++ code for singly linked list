#include <iostream>
#include <vector>
struct Node {
    int data;
    Node* next;
    Node(int val) : data(val), next(nullptr) {}
};
std::vector<int> printList(Node *head) {
    std::vector<int> ans;
    for (Node* i = head; i != nullptr; i = i->next) {
        ans.push_back(i->data);
    }
    return ans;
}
int main() {
    Node* head = new Node(1);
    head->next = new Node(2);
    head->next->next = new Node(3);
    head->next->next->next = new Node(4);
    std::vector<int> listElements = printList(head);
    std::cout << "Elements of the linked list: ";
    for (int val : listElements) {
        std::cout << val << " ";
    }
    std::cout << std::endl;
    Node* current = head;
    while (current != nullptr) {
        Node* nextNode = current->next;
        delete current;
        current = nextNode;
    }
    return 0;
}
