#include <iostream>
using namespace std;
class construct
{
    public:    
    float area;
    construct()
    {
        area=0;
    }
    construct(int a,int b)
    {
        area=a*b;
    }
    void display()
    {
        cout<<area<<endl;
    }
};
int main()
{
    construct c;
    construct c2(63,23);
    c.display();
    c2.display();
    return 0;
}
