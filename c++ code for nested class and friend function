#include <iostream>
#include <string>
using namespace std;
class student;
void putdata(student s1);
class student
{
public:
    int age;
    string name;
    void getdata() 
    {
        cout << "enter name and age:";
        cin >> name >> age;
    }
};
void putdata(student s1)
{
    cout << "Name: " << s1.name << endl;
    cout << "Age: " << s1.age << endl;
}
int main()
{
    student s1; 
    s1.getdata(); 
    putdata(s1); 
    return 0;
}
