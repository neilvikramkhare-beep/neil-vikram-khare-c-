#include <iostream>
#include <string>
using namespace std;
int main() 
{
    string s = "GeeksForGEEks"; 
    int vowels = 0, consonants = 0, digits = 0, specialCharacters = 0;
    for (int i = 0; i < (int)s.size(); i++) {
        char c = s[i];
        if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' ||
            c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U')
             {
            vowels++;
        }
        else if (c >= '0' && c <= '9') 
        {
            digits++;
        }
        else if ( (c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z') ) {
            consonants++;
        }
        else
         {
            specialCharacters++;
        }
    }
    cout << "Vowels: " << vowels << "\n";
    cout << "Consonants: " << consonants << "\n";
    cout << "Digits: " << digits << "\n";
    cout << "Special characters: " << specialCharacters << "\n";
    return 0;
}
