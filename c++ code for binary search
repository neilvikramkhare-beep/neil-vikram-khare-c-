#include <iostream> 
#include <vector>   
int binarySearch(const std::vector<int>& arr, int target) {
    int left = 0;             
    int right = arr.size() - 1; 
    while (left <= right) { 
        int mid = left + (right - left) / 2; 
        if (arr[mid] == target) { 
            return mid;           
        } else if (arr[mid] < target) { 
            left = mid + 1;            
        } else {                       
            right = mid - 1;            
        }
    }
    return -1;
}
int main() {
    std::vector<int> sortedArray = {2, 5, 8, 12, 16, 23, 38, 56, 72, 91};
    int targetElement = 23;
    std::cout << "Array: ";
    for (int num : sortedArray) {
        std::cout << num << " ";
    }
    std::cout << std::endl;
    int resultIndex = binarySearch(sortedArray, targetElement);
    if (resultIndex != -1) {
        std::cout << "Element " << targetElement << " found at index: " << resultIndex << std::endl;
    } else
     {
        std::cout << "Element " << targetElement << " not found in the array." << std::endl;
    }
    targetElement = 100;
    resultIndex = binarySearch(sortedArray, targetElement);
    if (resultIndex != -1) {
        std::cout << "Element " << targetElement << " found at index: " << resultIndex << std::endl;
    } else {
        std::cout << "Element " << targetElement << " not found in the array." << std::endl;
    }
    return 0;
}
