#include <iostream>
using namespace std;
class Animal
{
    public:
    void eat()
    {
        cout<<"animal eats."<<endl;
    }
    void sleeps()
    {
        cout<<"animal sleeps."<<endl;
    }
};
class Dog:public Animal
{
    public:
    void barks()
    {
        cout<<"dog barks."<<endl;
    }
};
int main()
{
    cout<<"Topic dogs:"<<endl;
    Dog d;
    d.barks();
    d.eat();
    d.sleeps();
    return 0;
}
