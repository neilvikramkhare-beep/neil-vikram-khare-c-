#include <iostream>
using namespace std;
class Demo
{
    private:
    int num;
    char ch;
    public:
    void setMyValues(int num, char ch)
    {
        this->num=num;
        this->ch=ch;
    }
    void displayMyValues()
    {
        cout<<num<<endl;
        cout<<ch;
    }
};
int main()
{
Demo d;
d.setMyValues(67,'A');
d.displayMyValues();
return 0;
}
