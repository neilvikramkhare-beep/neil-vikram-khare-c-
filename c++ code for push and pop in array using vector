#include <iostream>
#include<vector>
using namespace std;
int main() 
{
    vector<int>array={67,49,65,87,54};
    cout<<"size of array is:"<<array.size()<<endl;
    cout<<array[4]<<""<<array.at(4)<<endl;
    cout<<"first element of array:"<<array.front()<<endl;
    cout<<"last element of array:"<<array.back()<<endl;
    return 0;
}
