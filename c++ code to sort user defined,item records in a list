#include <iostream>
#include <list>
#include <cstdlib>
#include <ctime>
using namespace std;
int main() {
    srand(time(NULL)); 
    list<int> lst;
    int i;
    for (i = 0; i < 10; i++) {
        lst.push_back(rand());
    }
    cout << "Original contents:\n";
    list<int>::iterator p = lst.begin();
    while (p != lst.end()) {
        cout << *p << " ";
        p++;
    }
    cout << endl;
    lst.sort();
    cout << "Sorted contents:\n";
    p = lst.begin();
    while (p != lst.end()) {
        cout << *p << " ";
        p++;
    }
    cout << endl;
    return 0;
}
