#include <iostream>
#include <vector>
using namespace std;
void rotateLeftByOne(vector<int>& a) 
{
    if (a.empty())
    {
        return;
    }
    int firstElement = a[0]; 
    int n = a.size();
    for (int i = 0; i < n - 1; ++i)
    {
        a[i] = a[i + 1];
    }
    a[n - 1] = firstElement; 
}
int main() 
{
    vector<int> numbers = {1, 2, 3, 4, 5};
cout << "Original array: ";
    for (int num : numbers) 
    {
        cout << num << " ";
    }
    cout << endl;
    rotateLeftByOne(numbers);
    cout << "Array after one left rotation: ";
    for (int num : numbers) 
    {
    cout << num << " ";
    }
    cout << endl;
    return 0;
}
